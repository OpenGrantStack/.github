{
  "name": "grantready-cloud",
  "version": "1.0.0",
  "description": "Cloud-integrated mobile grant management core services and SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:build": "docker build -t grantready-cloud .",
    "docker:run": "docker-compose up -d",
    "generate:docs": "typedoc --out docs/api src/index.ts",
    "migrate": "node dist/migrations/run.js",
    "seed": "node dist/seeds/run.js",
    "health": "node dist/scripts/health-check.js"
  },
  "keywords": [
    "grant",
    "management",
    "government",
    "compliance",
    "workflow",
    "saas",
    "api",
    "mobile"
  ],
  "author": "GrantReady <engineering@grantready.cloud>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grantready/grantready-cloud.git"
  },
  "bugs": {
    "url": "https://github.com/grantready/grantready-cloud/issues"
  },
  "homepage": "https://github.com/grantready/grantready-cloud#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "express": "^4.18.0",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.0.0",
    "express-validator": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "bcrypt": "^5.1.0",
    "pg": "^8.10.0",
    "redis": "^4.6.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1",
    "dotenv": "^16.0.0",
    "axios": "^1.6.0",
    "joi": "^17.9.0",
    "uuid": "^9.0.0",
    "swagger-ui-express": "^5.0.0",
    "yaml": "^2.3.0",
    "node-cron": "^3.0.0",
    "bull": "^4.11.0",
    "aws-sdk": "^2.1400.0",
    "multer": "^1.4.5",
    "multer-s3": "^3.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/cors": "^2.8.0",
    "@types/compression": "^1.7.0",
    "@types/helmet": "^4.0.0",
    "@types/uuid": "^9.0.0",
    "@types/jest": "^29.5.0",
    "@types/supertest": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-security": "^1.5.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "supertest": "^6.0.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  }
}
